# AI Options Trader Configuration
# Copy this file to .env and customize for your setup

# IBKR Connection Settings
IBKR_HOST=127.0.0.1
IBKR_PORT=7497  # 7497 for paper trading, 7496 for live trading
IBKR_CLIENT_ID=1

# Trading Parameters
CONFIDENCE_THRESHOLD=0.8  # 80% minimum confidence to trade
TRADE_QUANTITY=1  # Number of contracts per trade
TRADING_INTERVAL_SEC=300  # 5 minutes between trading cycles

# Logging
LOG_LEVEL=INFO  # DEBUG, INFO, WARNING, ERROR
